    "@vitejs/plugin-vue": "^2.1.0",
    "@vue/compiler-sfc": "^3.0.5",
    "autoprefixer": "^10.4.0",
    "cypress": "^9.5.0",
    "cypress-fill-command": "^1.0.2",
    "cypress-plugin-tab": "^1.0.5",
    "eslint": "^7.21.0",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-vue": "^7.7.0",
    "jest": "^26.6.3",
    "node-fetch": "^2.6.1",
    "postcss": "^8.4.4",
