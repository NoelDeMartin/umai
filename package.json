{
  "name": "umai",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "deploy": "noeldemartin-deploy-app noeldemartin/umai umai.noeldemartin.com",
    "lint": "npm run lint:eslint",
    "lint:eslint": "eslint src && eslint cypress",
    "lint:ts": "tsc --noEmit",
    "lint:vue": "vue-tsc --noEmit",
    "test": "jest --verbose",
    "test:serve-app": "NODE_ENV=testing vite",
    "test:serve-pod": "community-solid-server -p 4000 -l warn",
    "test:coverage": "jest --coverage",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:test": "start-server-and-test test:serve-app http-get://localhost:3000 test:serve-pod http-get://localhost:4000 cy:run",
    "storybook": "start-storybook -p 6006 --no-manager-cache"
  },
  "dependencies": {
    "@noeldemartin/solid-utils": "next",
    "@noeldemartin/utils": "next",
    "buffer": "^6.0.3",
    "soukai": "next",
    "soukai-solid": "next",
    "tailwindcss": "^2.0.3",
    "vue": "^3.2.20",
    "vue-router": "^4.0.5",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.13.10",
    "@iconify/json": "^1.1.313",
    "@noeldemartin/eslint-config-vue": "next",
    "@noeldemartin/scripts": "next",
    "@solid/community-server": "^1.0.0",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/vue3": "^6.3.12",
    "@types/jest": "^26.0.20",
    "@types/node-fetch": "^2.5.10",
    "@vitejs/plugin-vue": "^1.1.5",
    "@vue/compiler-sfc": "^3.0.5",
    "autoprefixer": "^10.2.5",
    "cypress": "^7.5.0",
    "eslint": "^7.21.0",
    "eslint-plugin-vue": "^7.7.0",
    "idb": "^6.1.3",
    "jest": "^26.6.3",
    "node-fetch": "^2.6.1",
    "postcss": "^8.2.7",
    "start-server-and-test": "^1.12.3",
    "storybook-builder-vite": "^0.1.2",
    "ts-jest": "^26.5.3",
    "typescript": "^4.1.5",
    "unplugin-icons": "^0.12.15",
    "unplugin-vue-components": "^0.15.6",
    "vite": "^2.6.3",
    "vue-tsc": "^0.28.6"
  },
  "engines": {
    "node": ">=14.15.0 <15"
  }
}
