{
  "name": "umai",
  "version": "0.0.0",
  "private": true,
  "repository": "github:noeldemartin/umai",
  "scripts": {
    "dev": "vite",
    "dev:serve-pod": "community-solid-server -c @css:config/file-no-setup.json -p 4000 -f ./pod",
    "build": "vite build",
    "serve": "vite preview",
    "deploy": "noeldemartin-deploy-app noeldemartin/umai umai.noeldemartin.com",
    "lint": "noeldemartin-lint src cypress .storybook",
    "test": "jest --verbose",
    "test:serve-app": "NODE_ENV=testing vite",
    "test:serve-pod": "community-solid-server -p 4000 -l warn",
    "test:coverage": "jest --coverage",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:test": "start-server-and-test test:serve-app http-get://localhost:3000 test:serve-pod http-get://localhost:4000 cy:run",
    "storybook": "start-storybook -p 6006",
    "format": "npm run format:js && npm run format:ts && npm run format:vue",
    "format:js": "prettier-eslint --write \"src/**/*.js\" \"cypress/**/*.js\" \".storybook/**/*.js\" \"*.js\"",
    "format:ts": "prettier-eslint --parser typescript --write \"src/**/*.ts\" \"cypress/**/*.ts\" \".storybook/**/*.ts\" \"*.ts\"",
    "format:vue": "prettier-eslint --parser vue --write \"src/**/*.vue\""
  },
  "dependencies": {
    "@headlessui/vue": "^0.0.0-insiders.ab6310c",
    "@inrupt/solid-client-authn-browser": "^1.11.6",
    "@noeldemartin/solid-utils": "^0.1.1-next.58b73c94b4d8c9b5b8fe26c1f1eb97095f1af84a",
    "@noeldemartin/utils": "^0.2.1-next.e1d1a03c5f0e062d37212c66ff732fb7a73742ed",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/typography": "^0.5.0",
    "buffer": "^6.0.3",
    "dompurify": "^2.3.4",
    "idb": "^7.0.0",
    "image-blob-reduce": "^4.1.0",
    "marked": "^4.0.12",
    "soukai": "^0.4.1-next.269ba23199792c026b72bbe618e1d699a4c26c03",
    "soukai-solid": "^0.4.1-next.72e82e83d79bb96a76a50c609feb2f06b6deb991",
    "tailwindcss": "^3.0.0",
    "vue": "^3.2.29",
    "vue-i18n": "^9.2.0-beta.23",
    "vue-router": "^4.0.5",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.13.10",
    "@iconify/json": "^1.1.313",
    "@intlify/vite-plugin-vue-i18n": "^3.2.1",
