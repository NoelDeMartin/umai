@prefix schema: <https://schema.org/> .
@prefix purl: <http://purl.org/dc/terms/> .

<#it>
    a schema:ItemList ;
    schema:name "Alice's Public Recipes" ;
    purl:creator <https://pod.example.com/alice/profile/card#me> ;
    schema:itemListElement <#ramen>, <#pisto> .

<#ramen>
    a schema:ListItem ;
    schema:item <https://pod.example.com/recipes/ramen#it> .

<#pisto>
    a schema:ListItem ;
    schema:item <https://pod.example.com/recipes/pisto#it> .
